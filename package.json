{
  "name": "radiate",
  "version": "0.1.0",
  "description": "A responsive dashboard written in React - built for all screen sizes",
  "author": "Chris Wheatley <chriswheatley87@gmail.com>",
  "license": "MIT",
  "main": "src/lib/server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/chrisdwheatley/radiate.git"
  },
  "scripts": {
    "coverage": "babel-node node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha -- --recursive",
    "lint": "standard",
    "postinstall": "webpack -p --config webpack.prod.config.js",
    "start": "node src/lib/server.js",
    "test": "mocha --compilers js:babel-core/register --recursive"
  },
  "standard": {
    "ignore": [
      "test"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "keywords": [
    "radiate",
    "dashboard",
    "css grid layout",
    "grid dashboard",
    "information radiator",
    "wall dash",
    "wall dashboard",
    "wall display"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "cz-conventional-changelog": "^1.1.6",
    "enzyme": "^2.4.1",
    "isparta": "^4.0.0",
    "mocha": "^3.0.0",
    "sinon": "^1.17.5",
    "supertest": "^2.0.0",
    "webpack-dev-middleware": "^1.6.1"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "glare": "^0.2.0",
    "lodash.uniqueid": "^4.0.1",
    "markdown": "^0.5.0",
    "material-ui": "^0.15.2",
    "moment": "^2.10.6",
    "radium": "^0.18.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.18.0",
    "ua-parser-js": "^0.7.10",
    "webpack": "^1.12.14",
    "whatwg-fetch": "^1.0.0"
  }
}
